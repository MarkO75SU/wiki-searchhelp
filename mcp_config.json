{
  "mcpServers": {
    "filesystem": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-filesystem", "/pfad/zu/deinem/projekt"]
    },
    "postgres": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-postgres", "postgresql://postgres:[PASSWORD]@db.[PROJECT-ID].supabase.co:5432/postgres"]
    },
    "http": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-http"]
    },
    "google-maps": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-google-maps"],
      "env": {
        "GOOGLE_MAPS_API_KEY": "DEIN_API_KEY"
      }
    }
  }
}